    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "http://windows.php.net/downloads/pecl/releases/geoip/$version/php_geoip-$version-7.2-ts-vc15-x64.zip"
            },
            "32bit": {
                "url": "http://windows.php.net/downloads/pecl/releases/geoip/$version/php_geoip-$version-7.2-ts-vc15-x86.zip"
            }
        }
    },
    "uninstaller": {
        "file": "uninstall.ps1"
    }
}
