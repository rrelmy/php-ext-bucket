        "architecture": {
            "64bit": {
                "url": "https://windows.php.net/downloads/pecl/releases/apcu/$version/php_apcu-$version-7.3-ts-vc15-x64.zip"
            },
            "32bit": {
                "url": "https://windows.php.net/downloads/pecl/releases/apcu/$version/php_apcu-$version-7.3-ts-vc15-x86.zip"
            }
        }
    },
